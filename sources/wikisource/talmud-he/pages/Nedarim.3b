 <script type="text/javascript"> 

function toggle_acc()
{
	if (document.getElementById('slickbox').style.display=='none')
	{
	
	document.getElementById('slickbox').style.display='block';
	
	
	document.getElementById('rarrow').src='images_main/arrow_up_black.png';
	
	document.getElementById('int').style.color='#567c19';
	
	
	return;
	
	}
	
	if (document.getElementById('slickbox').style.display=='block')
	{
	
	document.getElementById('slickbox').style.display='none';

		document.getElementById('rarrow').src='images_main/arrow_down.png';
		
		
			document.getElementById('int').style.color='#fff';
	
	return;
	
	}
	
}

</script>
 
